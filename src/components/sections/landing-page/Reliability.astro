---
// Reliability section explaining WHOIS database update requirements
import Section from '../../ui/Section.astro';
import Title from '../../ui/Title.astro';
import Paragraph from '../../ui/Paragraph.astro';

const { title, paragraphs = [] } = Astro.props;
---

<Section backgroundColor="bg-white">
		<Title title={title} />
		
		<Paragraph>
			{paragraphs.map((paragraph: string) => <p set:html={paragraph} />)}
		</Paragraph>
</Section>

