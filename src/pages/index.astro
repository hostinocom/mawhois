---
import Layout from '../layouts/Layout.astro';
import Header from '../components/layout/Header.astro';
import Hero from '../components/sections/landing-page/Hero.astro';
import Description from '../components/sections/landing-page/Description.astro';
import ServiceInfo from '../components/sections/landing-page/ServiceInfo.astro';
import UseCases from '../components/sections/landing-page/UseCases.astro';
import PublicData from '../components/sections/landing-page/PublicData.astro';
import Privacy from '../components/sections/landing-page/Privacy.astro';
import Reliability from '../components/sections/landing-page/Reliability.astro';
import FAQ from '../components/ui/FAQ.astro';
import { faqsLandingPage } from '../config/faqsContent';
---
	
<Layout>
	<!-- Home Page -->
	<Header />

	<Hero />
	<Description />
	<ServiceInfo />
	<UseCases />
	<PublicData />
	<Privacy />
	<Reliability />
	<FAQ faqs={faqsLandingPage} title="FAQ - Whois Maroc" />
</Layout>
