---
import Layout from '../layouts/Layout.astro';
import Header from '../components/layout/Header.astro';
import Hero from '../components/sections/Hero.astro';
import Description from '../components/sections/Description.astro';
import ServiceInfo from '../components/sections/ServiceInfo.astro';
import UseCases from '../components/sections/UseCases.astro';
import PublicData from '../components/sections/PublicData.astro';
import Privacy from '../components/sections/Privacy.astro';
import Reliability from '../components/sections/Reliability.astro';
import FAQ from '../components/sections/FAQ.astro';
import DomainSearchSection from '../components/sections/DomainSearchSection.astro';
import { registries } from '../config/registries';
---
	
<Layout>
	<!-- Home Page -->
	<Header />
	<Hero registries={registries} />
	<Description />
	<ServiceInfo />
	<UseCases />
	<PublicData />
	<Privacy />
	<Reliability />
	<FAQ />
	<DomainSearchSection />
</Layout>
